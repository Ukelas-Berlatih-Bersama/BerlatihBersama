) => {\n    setBuka(false);\n  };\n\n  const handleSwitch = (e, newValue) => {\n    setToogle(newValue);\n  };\n\n  const body = (\n    <form onSubmit={addModule} className={classes.modal}>\n      <Typography variant=\"h6\">Tambah Materi</Typography>\n      <div style={{ marginTop: 15 }}>\n        <Typography style={{ marginBottom: -10 }}>Judul</Typography>\n        <TextField\n          variant=\"outlined\"\n          margin=\"normal\"\n          required\n          id=\"judul\"\n          label=\"judul\"\n          name=\"judul\"\n          autoComplete=\"judul\"\n          fullWidth\n          value={materi}\n          onChange={(e) => setMateri(e.target.value)}\n        ></TextField>\n      </div>\n      <div style={{ margin: \"15px 0 15px 0\" }}>\n        <div style={{ display: \"flex\", marginBottom: -10 }}>\n          <Typography style={{ marginRight: 5 }}>Deskripsi</Typography>\n          <Typography style={{ color: \"GrayText\" }}>(Optional)</Typography>\n        </div>\n        <TextField\n          variant=\"outlined\"\n          margin=\"normal\"\n          required\n          id=\"description\"\n          label=\"Deskripsi\"\n          name=\"description\"\n          value={desc}\n          onChange={(e) => setDesc(e.target.value)}\n        ></TextField>\n      </div>\n      <div style={{ marginBottom: 5 }}>\n        <div style={{ display: \"flex\", marginBottom: -10 }}>\n          <Typography style={{ marginRight: 5 }}>URL Video/Youtube</Typography>\n          <Typography style={{ color: \"GrayText\" }}>(Optional)</Typography>\n        </div>\n        <TextField\n          variant=\"outlined\"\n          margin=\"normal\"\n          id=\"video\"\n          label=\"Optional\"\n          name=\"video\"\n          value={video}\n          onChange={(e) => setVideo(e.target.value)}\n        ></TextField>\n      </div>\n      <div style={{ display: \"flex\", marginBottom: 1 }}>\n        <Typography style={{ marginRight: 5 }}>Upload Foto</Typography>\n        <Typography style={{ color: \"GrayText\" }}>(Optional)</Typography>\n      </div>\n      <div style={{ display: \"flex\", marginBottom: 10 }}>\n        <Input type=\"file\" onChange={handleUpload}></Input>\n      </div>\n      <ButtonGroup style={{ display: \"flex\", justifyContent: \"flex-end\" }}>\n        <Button variant=\"text\" size=\"small\" color=\"primary\" type=\"submit\">\n          Tambah\n        </Button>\n        <Button\n          variant=\"text\"\n          size=\"small\"\n          color=\"secondary\"\n          onClick={handleClose}\n        >\n          Tutup\n        </Button>\n      </ButtonGroup>\n    </form>\n  );\n\n  return (\n    <>\n      {status == \"success\" ? (\n        <div>\n          <Navbar />\n          <main style={{ flexGrow: 1, padding: 30 }}>\n            <Container className={classes.container}>\n              <ButtonGroup\n                style={{ display: \"flex\", justifyContent: \"space-between\" }}\n              >\n                <Button\n                  startIcon={<ArrowBack />}\n                  size=\"small\"\n                  variant=\"text\"\n                  color=\"primary\"\n                  onClick={handleGoBack}\n                >\n                  Kembali ke Daftar Mata Pelajaran\n                </Button>\n                <Button\n                  startIcon={<Settings />}\n                  size=\"small\"\n                  variant=\"text\"\n                  color=\"primary\"\n                  onClick={handleBuka}\n                >\n                  Edit Mata Pelajaran\n                </Button>\n                <Modal\n                  open={buka}\n                  onClose={handleTutup}\n                  style={{\n                    display: \"flex\",\n                    alignItems: \"center\",\n                    justifyContent: \"center\",\n                  }}\n                >\n                  <form onSubmit={handleEditModule} className={classes.modal}>\n                    <Typography variant=\"h6\">Edit Mata Pelajaran</Typography>\n                    <TextField\n                      variant=\"outlined\"\n                      margin=\"normal\"\n                      id=\"name\"\n                      label=\"Edit Nama Mata Pelajaran\"\n                      name=\"name\"\n                      // required\n                      fullWidth\n                      value={edit}\n                      onChange={(e) => setEdit(e.target.value)}\n                      style={{ margin: \"20px 0 20px 0\" }}\n                    ></TextField>\n                    <ButtonGroup\n                      style={{ display: \"flex\", justifyContent: \"flex-end\" }}\n                    >\n                      <Button\n                        variant=\"text\"\n                        color=\"primary\"\n                        onClick={handleTutup}\n                      >\n                        Batal\n                      </Button>\n                      <Button\n                        style={{ color: \"GrayText\" }}\n                        variant=\"text\"\n                        type=\"submit\"\n                      >\n                        Simpan Perubahan\n                      </Button>\n                    </ButtonGroup>\n                  </form>\n                </Modal>\n              </ButtonGroup>\n              <div>\n                {subject.classroom.nodes.map((node, i) => {\n                  return (\n                    <Typography key={i} variant=\"h4\" style={{ padding: 10 }}>\n                      {node.displayField} / {subject.name}\n                    </Typography>\n                  );\n                })}\n              </div>\n              <Grid className={classes.root}>\n                <Tabs\n                  value={toogle}\n                  onChange={handleSwitch}\n                  indicatorColor=\"primary\"\n                  textColor=\"primary\"\n                  centered\n                >\n                  