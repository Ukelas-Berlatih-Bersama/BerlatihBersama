                    <TextField\n                      variant=\"outlined\"\n                      margin=\"normal\"\n                      id=\"name\"\n                      label=\"Edit Materi\"\n                      name=\"name\"\n                      // required\n                      fullWidth\n                      value={edit}\n                      onChange={(e) => setEdit(e.target.value)}\n                      style={{ margin: \"20px 0 20px 0\" }}\n                    ></TextField>\n                    <ButtonGroup\n                      style={{ display: \"flex\", justifyContent: \"flex-end\" }}\n                    >\n                      <Button\n                        variant=\"text\"\n                        color=\"primary\"\n                        onClick={handleTutup}\n                      >\n                        Batal\n                      </Button>\n                      <Button\n                        style={{ color: \"GrayText\" }}\n                        variant=\"text\"\n                        type=\"submit\"\n                      >\n                        Simpan Perubahan\n                      </Button>\n                    </ButtonGroup>\n                  </form>\n                </Modal>\n              </ButtonGroup>\n              <div>\n                <Typography variant=\"h4\" style={{ padding: 10 }}>\n                  {materi.title}\n                </Typography>\n              </div>\n              <Grid className={classes.root}>\n                <Tabs\n                  value={toogle}\n                  onChange={handleSwitch}\n                  indicatorColor=\"primary\"\n                  textColor=\"primary\"\n                  centered\n                >\n                  <Tab label=\"Tugas\" style={{ width: 400 }} />\n                </Tabs>\n              </Grid>\n              <div>\n                <div className={classes.add}>\n                  <Typography variant=\"h6\">\n                    Semua Tugas ({materi.assignment.totalCount})\n                  </Typography>\n                  <TextField\n                    label=\"Cari tugas\"\n                    InputProps={{\n                      startAdornment: (\n                        <InputAdornment position=\"start\">\n                          <Search />\n                        </InputAdornment>\n                      ),\n                    }}\n                    variant=\"outlined\"\n                    size=\"small\"\n                    onChange={(e) => {\n                      setSearchTerm(e.target.value);\n                    }}\n                  />\n                  <Button\n                    variant=\"contained\"\n                    color=\"primary\"\n                    onClick={handleOpen}\n                    style={{ width: \"15%\" }}\n                  >\n                    Tambah Tugas\n                  </Button>\n                  <Modal\n                    open={open}\n                    onClose={handleClose}\n                    style={{\n                      display: \"flex\",\n                      alignItems: \"center\",\n                      justifyContent: \"center\",\n                    }}\n                  >\n                    {body}\n                  </Modal>\n                </div>\n                <div className={classes.mod}>\n                  <Grid container>\n                    {materi.assignment.nodes\n                      .filter((node) =>\n                        node.displayField\n                          .toLowerCase()\n                          .includes(searchTerm.toLowerCase())\n                      )\n                      .map((node, idx) => {\n                        // const sub = node.displayField;\n                        return (\n                          <Grid container direction=\"column\">\n                            <CardAssignment node={node} key={idx} />\n                          </Grid>\n                        );\n                